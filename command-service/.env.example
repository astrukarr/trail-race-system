# Command Service (.env.example)
PORT=3001
NODE_ENV=development

# PostgreSQL (use Neon/Render/DO in production)
DATABASE_URL=postgresql://trail_race_user:trail_race_password@localhost:5432/trail_race_db

# RabbitMQ (use CloudAMQP in production)
RABBITMQ_URL=amqp://trail_race_user:trail_race_password@localhost:5672

# JWT
JWT_SECRET=change-me-in-production
JWT_EXPIRES_IN=24h
JWT_ISSUER=trail-race-system

# CORS
CORS_ORIGIN=http://localhost:3000

# Logging
LOG_LEVEL=info

# Rate limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX=100
